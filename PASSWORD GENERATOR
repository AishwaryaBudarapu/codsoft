import random
import string
def generate_paassword(length):
characters=string.asci_letters + string.digits + string.punctuation
password = ' '.join(random.choice(characters) for_in range(length))
return password
  def main():
try:
 length = int(input("Enter the desired length of password: " ))
 password = generate_password(length)
 print("Generate password :",  password)
 expect ValueError:
print("Please Enter a Valid Length for password :")
if__name__==__main__":
main()
